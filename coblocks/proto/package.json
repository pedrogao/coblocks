{
  "name": "@coblocks/proto",
  "version": "0.0.1",
  "description": "proto files",
  "author": "pedrogao",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "npm run clean && npm run protoc",
    "clean": "rm ./*.ts",
    "protoc": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto.cmd --ts_proto_out=./ --ts_proto_opt=nestJs=true --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=outputServices=grpc-js --proto_path=./ project.proto"
  },
  "dependencies": {
    "@nestjs/microservices": "^10.2.9",
    "@grpc/grpc-js": "^1.9.11",
    "@grpc/proto-loader": "^0.7.10",
    "ts-proto": "^1.164.0"
  },
  "devDependencies": {}
}
